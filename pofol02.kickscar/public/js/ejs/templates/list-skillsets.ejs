<% 
for(skillset in skills) {
%>
<div class="skillset__item">
    <h3 class="skillset__title"><%=skillset%></h3>
    <% 
        for(let i = 0; i < skills[skillset].length; i++) {
            const skill = skills[skillset][i];
    %>
        <div class="skill">
            <div class="skill__description">
                <span><%=skill.name %></span>
                <span>Lvl. <%=skill.level %></span>
            </div>
            <div class="skill__bar">
                <div class="skill__value" style="width: <%=skill.level * 100 / 5 %>%"></div>
            </div>
        </div>
    <%
        }
    %>
</div>
<%
}
%>