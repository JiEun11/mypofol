    <%
        }
    %>
</div>
<%
}
%>

<%
if(trainings["language"]) {
%>
<div class="training">
    <div class="training">
        <h4><i class="fa-solid fa-language"></i> 외국어</h4>
        <%
            for(let i = 0; i < trainings.language.length; i++) {
        %>
            <div class="training-item">
                <div class="training__content">
