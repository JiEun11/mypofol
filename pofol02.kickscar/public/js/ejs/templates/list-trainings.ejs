<%
if(trainings["certificate"]) {
%>
<div class="training certificate">
    <h4><i class="fa-solid fa-certificate"></i> 자격증</h4>
    <%
        for(let i = 0; i < trainings.certificate.length; i++) {
    %>
        <div class="training-item">
            <div class="training__content">
                <p class="training__name"><%=trainings.certificate[i].title %></p>
                <p class="training__period"><%=trainings.certificate[i].awarded %></p>
                <p class="training__description"><%=trainings.certificate[i].description %></p>
            </div>
        </div>
    <%
        }
    %>
</div>
<%
}
%>

<%
if(trainings["education"]) {
%>
<div class="training">
