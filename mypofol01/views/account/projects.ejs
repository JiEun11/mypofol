                                    <div class="project-name">
                                        <p>Java / Spring</p><p>Lvl. 4</p>
                                    </div>
                                    <div class="progress">
                                        <div class="progress-bar" role="progressbar" aria-valuenow="4" aria-valuemin="0" aria-valuemax="5" style="width: calc((4/5)*100%)"></div>
                                    </div>

                                    <div class="project-name">
                                        <p>Node.js / Express</p><p>Lvl. 3</p>
                                    </div>
                                    <div class="progress">
                                        <div class="progress-bar" role="progressbar" aria-valuenow="3" aria-valuemin="0" aria-valuemax="5" style="width: calc((3/5)*100%)"></div>
